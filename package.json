{
  "name": "fix-tsup-cjs",
  "version": "1.2.0",
  "description": "Fix the commonjs default export statement and type definition of tsup output",
  "keywords": [
    "tsup",
    "cjs",
    "postbuild",
    "commonjs",
    "commonjs dts",
    "commonjs types",
    "module.exports",
    "export default"
  ],
  "repository": {
    "url": "https://github.com/u3u/fix-tsup-cjs.git"
  },
  "license": "MIT",
  "author": "u3u <qwq@qwq.cat> (https://github.com/u3u)",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "pnpm build",
    "test": "vitest --ui --coverage"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --concurrent false"
  },
  "lint-staged": {
    "*.{js,cjs,ts}": [
      "eslint --fix"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "cac": "^6.7.14",
    "fast-glob": "^3.3.2",
    "kolorist": "^1.8.0",
    "read-pkg": "^8.1.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^18.19.59",
    "@u3u/eslint-config": "^2.15.2",
    "@u3u/prettier-config": "^3.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.7",
    "eslint": "^8.57.1",
    "execa": "^7.2.0",
    "fix-tsup-cjs": "link:",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^7.2.0",
    "typescript": "^5.6.3",
    "vitest": "^0.34.6"
  },
  "packageManager": "pnpm@8.15.9",
  "publishConfig": {
    "access": "public"
  }
}
