{
  "name": "fix-tsup-cjs",
  "version": "1.1.0",
  "description": "Fix the commonjs default export statement and type definition of tsup output",
  "keywords": [
    "tsup",
    "cjs",
    "postbuild",
    "commonjs",
    "commonjs dts",
    "commonjs types",
    "module.exports",
    "export default"
  ],
  "repository": {
    "url": "https://github.com/u3u/fix-tsup-cjs.git"
  },
  "license": "MIT",
  "author": "u3u <qwq@qwq.cat> (https://github.com/u3u)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "pnpm build"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "cac": "^6.7.14",
    "fast-glob": "^3.2.12",
    "kolorist": "^1.8.0",
    "read-pkg": "^8.0.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.0",
    "@types/node": "^18.16.3",
    "@u3u/prettier-config": "^1.0.2",
    "fix-tsup-cjs": "link:",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "packageManager": "pnpm@8.4.0",
  "publishConfig": {
    "access": "public"
  }
}
